  "description": "This is a social networking project created using MERN stack",
  "main": "index.js",
  "scripts": {
    "development": "nodemon",
    "heroku-postbuild": "cd client && npm --production=false && npm run build",
    "dev":"concurrently \"forever -m 5 -w bin/www\" \"cd client && npm start\"",
    "build": "webpack --config webpack.config.client.production.js && webpack --mode=production --config webpack.config.server.js",
    "start": "SET NODE_ENV=production node ./dist/server.generated.js"
